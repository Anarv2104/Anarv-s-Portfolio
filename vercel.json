{
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "installCommand": "npm install --force",
  "outputDirectory": ".next",
  "functions": {
    "pages/api/**/*.ts": {
      "maxDuration": 30
    },
    "pages/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, s-maxage=31536000, immutable"
        },
        {
          "key": "Content-Encoding",
          "value": "gzip, br"
        },
        {
          "key": "Vary",
          "value": "Accept-Encoding, Accept"
        }
      ]
    },
    {
      "source": "/_next/image(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, s-maxage=31536000, immutable"
        }
      ]
    },
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "env": {
    "NODE_OPTIONS": "--max-old-space-size=4096",
    "NEXT_SHARP": "1"
  },
  "build": {
    "env": {
      "NODE_OPTIONS": "--max-old-space-size=4096",
      "NEXT_SHARP": "1"
    }
  }
}